{
  "orchis": {
    "app": {
      "port": 7770,
      "serverVerticles": 1,
      "rules": "$ref:rules"
    },
    "jwt": {
      "secret": "secret",
      "issuer": "test-orchis-api-gateway",
      "expiresIn": "PT60S"
    }
  },
  "registry:system": {
    "symmetric-jwt-service": {
      "main": "com.cloudentity.edge.jwt.impl.SymmetricJwtService"
    },
    "jwt-service": {
      "main": "com.cloudentity.tools.vertx.jwt.impl.JwtServiceVerticle"
    }
  },
  "registry:request-plugins": {
    "authn": { "main": "com.cloudentity.edge.plugin.impl.authn.AuthnPlugin" },
    "authn-edge": { "main": "com.cloudentity.edge.plugin.authn.TestAuthnEdgePlugin" },
    "authn-proxy": { "main": "com.cloudentity.edge.plugin.impl.authn.AuthnProxyPlugin" },
    "ping": { "main": "com.cloudentity.edge.plugin.impl.test.PingOriginalPlugin" }
  },
  "authn": {
    "methodsMapping": {
      "jwt": "jwtAuthnProvider"
    },
    "entitiesMapping": {
    }
  },
  "authn-edge": {
    "path": "/authn-proxy",
    "http": {
      "serviceLocation": {
        "host": "localhost",
        "port": 7770,
        "ssl": false
      }
    }
  },
  "registry:authn-method-providers": {
    "jwtAuthnProvider": {
      "main": "com.cloudentity.edge.plugin.impl.authn.methods.JwtAuthnProvider",
      "verticleConfig": {
        "jwtServiceAddress": "symmetric"
      }
    }
  },
  "jwt-service": {
    "secret": "secret",
    "issuer": "orchis-api-gateway",
    "expiresIn": "PT60S"
  },
  "symmetric-jwt-service": {
    "secret": "secret",
    "issuer": "orchis-api-gateway",
    "expiresIn": "PT60S"
  }
}
