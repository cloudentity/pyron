{
  "rules": [
    {
      "default": {
        "pathPrefix": "/sample-rs",
        "targetService": "sample-rs",
        "dropPrefix": true,
        "requestPlugins": [
          {
            "name": "authn",
            "conf": {
              "methods": [
                "oauth10"
              ],
              "entities": []
            }
          }
        ]
      },
      "endpoints": [
        {
          "method": "GET",
          "pathPattern": "/test"
        },
        {
          "method": "POST",
          "pathPattern": "/test"
        }
      ]
    }
  ]
}