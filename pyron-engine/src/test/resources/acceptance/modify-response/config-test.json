{
  "app": {
    "port": 8080,
    "rules": [
      {
        "default": {
          "targetHost": "localhost",
          "targetPort": 8201,
          "pathPrefix": "",
          "dropPrefix": false
        },
        "endpoints": [
          {
            "method": "GET",
            "pathPattern": "/modify-response",
            "requestPlugins": [
              {
                "name": "modify-response",
