{
  "type": "vault-keycerts-ext",
  "format": "json",
  "config": {
    "host": "$env:CONFIG_STORE_SSL_VAULT_KEYCERTS__VAULT_HOST:string",
    "port": "$env:CONFIG_STORE_SSL_VAULT_KEYCERTS__VAULT_PORT:int",
    "keyCertPath": "$env:CONFIG_STORE_SSL_VAULT_KEYCERTS__VAULT_PATH:string:/pyron/keycerts/global",
    "vaultToken": "$env:CONFIG_STORE_SSL_VAULT_KEYCERTS__VAULT_SECRET_TOKEN:string",
    "http": {
      "ssl": "$env:CONFIG_STORE_SSL_VAULT_KEYCERTS__VAULT_SSL:boolean",
      "pemTrustOptions": {
        "certValues": ["$env:CONFIG_STORE_SSL_VAULT_KEYCERTS__ROOT_CA:string"]
      }
    },
    "output": {
      "keys": "keyValues",
      "certs": "certValues"
    },
    "ext": {
      "outputPath": "app.server.http.pemKeyCertOptions"
    },
    "fallback": {}
  }
}