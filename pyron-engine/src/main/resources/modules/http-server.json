{
  "app": {
    "server": {
      "http": {
        "port": "$env:?HTTP_SERVER_PORT:int:8080",
        "host": "$env:?HTTP_SERVER_HOST:string",
        "acceptBacklog": "$env:?HTTP_SERVER_ACCEPT_BACKLOG:int",
        "clientAuth": "$env:?HTTP_SERVER_CLIENT_AUTH:string",
        "clientAuthRequired": "$env:?HTTP_SERVER_CLIENT_AUTH_REQUIRED:boolean",
        "sni": "$env:?HTTP_SERVER_SNI:boolean",
        "sendBufferSize": "$env:?HTTP_SERVER_SEND_BUFFER_SIZE:int",
        "receiveBufferSize": "$env:?HTTP_SERVER_RECEIVE_BUFFER_SIZE:int",
        "trafficClass": "$env:?HTTP_SERVER_TRAFFIC_CLASS:int",
        "reuseAddress": "$env:?HTTP_SERVER_REUSE_ADDRESS:boolean",
        "logActivity": "$env:?HTTP_SERVER_LOG_ACTIVITY:boolean",
        "reusePort": "$env:?HTTP_SERVER_REUSE_PORT:boolean",
        "tcpNoDelay": "$env:?HTTP_SERVER_TCP_NO_DELAY:boolean",
        "tcpKeepAlive": "$env:?HTTP_SERVER_TCP_KEEP_ALIVE:boolean",
        "soLinger": "$env:?HTTP_SERVER_SO_LINGER:int",
        "usePooledBuffers": "$env:?HTTP_SERVER_USE_POOLED_BUFFERS:boolean",
        "idleTimeout": "$env:?HTTP_SERVER_IDLE_TIMEOUT:int",
        "ssl": "$env:?HTTP_SERVER_SSL:boolean",
        "enabledCipherSuites": "$env:?HTTP_SERVER_ENABLED_CIPHER_SUITES:array",
        "useAlpn": "$env:?HTTP_SERVER_USE_ALPN:boolean",
        "sessionCacheEnabled": "$env:?HTTP_SERVER_SESSION_CACHE_ENABLED:boolean",
        "enabledSecureTransportProtocols": "$env:?HTTP_SERVER_ENABLED_SECURE_TRANSPORT_PROTOCOLS:array",
        "tcpFastOpen": "$env:?HTTP_SERVER_TCP_FAST_OPEN:boolean",
        "tcpCork": "$env:?HTTP_SERVER_TCP_CORK:boolean",
        "tcpQuickAck": "$env:?HTTP_SERVER_TCP_QUICK_ACK:boolean",
        "compressionSupported": "$env:?HTTP_SERVER_COMPRESSION_SUPPORTED:boolean",
        "compressionLevel": "$env:?HTTP_SERVER_COMPRESSION_LEVEL:int",
        "maxWebsocketFrameSize": "$env:?HTTP_SERVER_MAX_WEBSOCKET_FRAME_SIZE:int",
        "maxWebsocketMessageSize": "$env:?HTTP_SERVER_MAX_WEBSOCKET_MESSAGE_SIZE:int",
        "websocketSubProtocols": "$env:?HTTP_SERVER_WEBSOCKET_SUB_PROTOCOLS:string",
        "handle100ContinueAutomatically": "$env:?HTTP_SERVER_HANDLE_100_CONTINUE_AUTOMATICALLY:boolean",
        "maxChunkSize": "$env:?HTTP_SERVER_MAX_CHUNK_SIZE:int",
        "maxInitialLineLength": "$env:?HTTP_SERVER_MAX_INITIAL_LINE_LENGTH:int",
        "maxHeaderSize": "$env:?HTTP_SERVER_MAX_HEADER_SIZE:int",
        "initialSettings": {
          "_nullify": true,
          "headerTableSize": "$env:?HTTP_SERVER_INITIAL_SETTINGS__HEADER_TABLE_SIZE:int",
          "pushEnabled": "$env:?HTTP_SERVER_INITIAL_SETTINGS__PUSH_ENABLED:boolean",
          "maxConcurrentStreams": "$env:?HTTP_SERVER_INITIAL_SETTINGS__MAX_CONCURRENT_STREAMS:int",
          "initialWindowSize": "$env:?HTTP_SERVER_INITIAL_SETTINGS__INITIAL_WINDOW_SIZE:int",
          "maxFrameSize": "$env:?HTTP_SERVER_INITIAL_SETTINGS__MAX_FRAME_SIZE:int",
          "maxHeaderListSize": "$env:?HTTP_SERVER_INITIAL_SETTINGS__MAX_HEADER_LIST_SIZE:int",
          "extraSettings": "$env:?HTTP_SERVER_INITIAL_SETTINGS__EXTRA_SETTINGS:object"
        },
        "alpnVersions": "$env:?HTTP_SERVER_ALPN_VERSIONS:array",
        "http2ConnectionWindowSize": "$env:?HTTP_SERVER_HTTP2_CONNECTION_WINDOW_SIZE:int",
        "decompressionSupported": "$env:?HTTP_SERVER_DECOMPRESSION_SUPPORTED:boolean",
        "acceptUnmaskedFrames": "$env:?HTTP_SERVER_ACCEPT_UNMASKED_FRAMES:boolean",
        "decoderInitialBufferSize": "$env:?HTTP_SERVER_DECODER_INITIAL_BUFFER_SIZE:int",
        "crlPaths": "$env:?HTTP_SERVER_CRL_PATHS:array",
        "crlValues": "$env:?HTTP_SERVER_CRL_VALUES:array",
        "keyStoreOptions": {
          "_nullify": true,
          "password": "$env:?HTTP_SERVER_KEY_STORE_OPTIONS__PASSWORD:string",
          "path": "$env:?HTTP_SERVER_KEY_STORE_OPTIONS__PATH:string",
          "value": "$env:?HTTP_SERVER_KEY_STORE_OPTIONS__VALUE:string"
        },
        "trustStoreOptions": {
          "_nullify": true,
          "password": "$env:?HTTP_SERVER_TRUST_STORE_OPTIONS__PASSWORD:string",
          "path": "$env:?HTTP_SERVER_TRUST_STORE_OPTIONS__PATH:string",
          "value": "$env:?HTTP_SERVER_TRUST_STORE_OPTIONS__VALUE:string"
        },
        "pemKeyCertOptions": {
          "_nullify": true,
          "keyPath": "$env:?HTTP_SERVER_PEM_KEY_CERT_OPTIONS__KEY_PATH:string",
          "keyPaths": "$env:?HTTP_SERVER_PEM_KEY_CERT_OPTIONS__KEY_PATHS:array",
          "keyValue": "$env:?HTTP_SERVER_PEM_KEY_CERT_OPTIONS__KEY_VALUE:string",
          "keyValues": "$env:?HTTP_SERVER_PEM_KEY_CERT_OPTIONS__KEY_VALUES:array",
          "certPath": "$env:?HTTP_SERVER_PEM_KEY_CERT_OPTIONS__CERT_PATH:string",
          "certPaths": "$env:?HTTP_SERVER_PEM_KEY_CERT_OPTIONS__CERT_PATHS:array",
          "certValue": "$env:?HTTP_SERVER_PEM_KEY_CERT_OPTIONS__CERT_VALUE:string",
          "certValues": "$env:?HTTP_SERVER_PEM_KEY_CERT_OPTIONS__CERT_VALUES:array"
        },
        "pemTrustOptions": {
          "_nullify": true,
          "certPaths": "$env:?HTTP_SERVER_PEM_TRUST_OPTIONS__CERT_PATHS:array",
          "certValues": "$env:?HTTP_SERVER_PEM_TRUST_OPTIONS__CERT_VALUES:array"
        },
        "pfxKeyCertOptions": {
          "_nullify": true,
          "password": "$env:?HTTP_SERVER_PFX_KEY_CERT_OPTIONS__PASSWORD:string",
          "path": "$env:?HTTP_SERVER_PFX_KEY_CERT_OPTIONS__PATH:string",
          "value": "$env:?HTTP_SERVER_PFX_KEY_CERT_OPTIONS__VALUE:string"
        },
        "pfxTrustOptions": {
          "_nullify": true,
          "password": "$env:?HTTP_SERVER_PFX_TRUST_OPTIONS__PASSWORD:string",
          "path": "$env:?HTTP_SERVER_PFX_TRUST_OPTIONS__PATH:string",
          "value": "$env:?HTTP_SERVER_PFX_TRUST_OPTIONS__VALUE:string"
        },
        "openSslEngineOptions": {
          "_nullify": true,
          "sessionCacheEnabled": "$env:?HTTP_SERVER_OPEN_SSL_ENGINE_OPTIONS__SESSION_CACHE_ENABLED:boolean"
        },
        "jdkSslEngineOptions": "$env:?HTTP_SERVER_JDK_SSL_ENGINE_OPTIONS:object"
      }
    }
  }
}